@model IEnumerable<StudentManagement.Models.Student>
<h1>Students List</h1>
<h2>Rafael Barroso Rodrigues</h2>
<h2>200568204</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Age</th>
            <th>Course</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
                <td>@student.Course</td>
            </tr>
        }
    </tbody>
</table>